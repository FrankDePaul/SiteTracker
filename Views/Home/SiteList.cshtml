@model ViewModel
<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <nav class="navbar navbar-light bg-light justify-content-between">
        <a href="/"  role="/Index">Home</a>
        

 
    </nav>



<table class="table">
    <thead>
        <tr>
            <th><a href="/SiteList/1">Site #</a></th>
            <th><a href="/SiteList/2">Enb #</a></th>
            <th><a href="/SiteList/3">Enb2 #</a></th>
            <th><a href="/SiteList/4">Four Dig #</a></th>
            <th><a href="/SiteList/5">Site Name</a></th>
           
          
            
           
            <th scope="col">Tech Name</th>
           
        </tr>
    </thead>
    <tbody>
        
        @foreach(var site in @ViewBag.AllSites )
        {
            <tr>
                
                <td><a href="/ShowSiteInfo/@site.Id">@site.Site_Number</a></td>
                <td>@site.Enb_Number</td>
                <td>@site.Enb2_Number</td>
                <td>@site.FourDigit_Number</td>
               

                <td>@site.Site_Name</td>
                <td>@site.Tech_Name</td>
              
            </tr>
        }
    </tbody>
</table>